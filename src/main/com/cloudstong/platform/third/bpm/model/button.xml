<?xml version="1.0" encoding="UTF-8"?>
<bpm>
	<buttons>
		<!-- class="listbutton" operatortype表示按钮的操作类型 ； script表示是否可添加脚本， 0不添加，1添加；  text为按钮名称 ； init表示是否默认初始化，0不初始化，1初始化  -->
		<!-- type=0代表起始节点 -->
		<button type="0" class="listbutton" operatortype="1" script="1" init="1" text="启动流程" />
		<button type="0" class="listbutton" operatortype="2" script="0" init="1" text="流程图" />
		<button type="0" class="listbutton" operatortype="3" script="0" init="1" text="打印" />
		<button type="0" class="listbutton" operatortype="6" script="1" init="1" text="保存草稿" />
		<!-- 其他节点 -->
		<!-- type=1代表第一个节点 -->
		<button type="1" class="listbutton" operatortype="1" script="1" init="1" text="提交" />
		<button type="1" class="listbutton" operatortype="8" script="1" init="1" text="保存表单" />
		<button type="1" class="listbutton" operatortype="9" script="0" init="1" text="流程图" />
		<button type="1" class="listbutton" operatortype="10" script="0" init="1" text="打印" />
		<button type="1" class="listbutton" operatortype="11" script="0" init="1" text="审批历史" />
		<!-- type=2为会签专有按钮 -->
		<button type="2" class="listbutton" operatortype="3" script="1" init="0" text="弃权" />
		<button type="2" class="listbutton" operatortype="7" script="1" init="0" text="补签" />
		<!-- type=3为普通节点专有按钮 -->
		<button type="3" class="listbutton" operatortype="17" script="1" init="0" text="加签" />
		<!-- type=4代表普通按钮 -->
		<button type="4" class="listbutton" operatortype="1" script="1" init="1" text="同意" />
		<button type="4" class="listbutton" operatortype="2" script="1" init="0" text="反对" />
		<button type="4" class="listbutton" operatortype="4" script="1" init="0" text="驳回" />
		<button type="4" class="listbutton" operatortype="5" script="1" init="1" text="驳回到发起人" />
		<button type="4" class="listbutton" operatortype="6" script="1" init="1" text="转交代办" />
		<button type="4" class="listbutton" operatortype="8" script="1" init="1" text="保存表单" />
		<button type="4" class="listbutton" operatortype="9" script="0" init="1" text="流程图" />
		<button type="4" class="listbutton" operatortype="10" script="0" init="1" text="打印" />
		<button type="4" class="listbutton" operatortype="11" script="0" init="1" text="审批历史" />
<!-- 		<button type="4" class="listbutton" operatortype="14" script="0" init="0" text="Web签章" /> -->
<!-- 		<button type="4" class="listbutton" operatortype="15" script="0" init="0" text="手写签章" /> -->
<!-- 		<button type="4" class="listbutton" operatortype="16" script="0" init="1" text="沟通" /> -->
	</buttons>
</bpm>
